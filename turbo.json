{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "build/**", ".react-router/**", ".docusaurus/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"outputs": []
		},
		"test:ci": {
			"outputs": ["coverage/**"]
		},
		"lint": {},
		"format": {},
		"types": {
			"dependsOn": ["^build"]
		},
		"db:gen": {
			"cache": false
		},
		"db:migrate": {
			"cache": false
		}
	}
}
